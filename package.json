{
  "name": "wholesale-test-site",
  "version": "1.0.0",
  "description": "Cypress tests for Sauce Demo website",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:ci:chrome": "start-server-and-test 'https://www.saucedemo.com' cy:chrome-parallel",
    "test:ci:firefox": "start-server-and-test 'https://www.saucedemo.com' cy:firefox-parallel",
    "test:ci:electron": "start-server-and-test 'https://www.saucedemo.com' cy:electron-parallel",
    "cy:chrome-parallel": "cypress run --browser chrome --record --parallel --group chrome-parallel",
    "cy:firefox-parallel": "cypress run --browser firefox --record --parallel --group firefox-parallel",
    "cy:electron-parallel": "cypress run --record --parallel --group electron-parallel"
  },
  "keywords": [
    "cypress",
    "testing",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^13.6.4",
    "start-server-and-test": "^2.0.3"
  }
}
