{
  "name": "practice-cypress",
  "version": "1.0.0",
  "description": "Cypress tests for practice website using Cypress and Cucumber",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:parallel": "./run-parallel-browsers.sh",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:electron": "cypress run --browser electron",
    "cy:open": "cypress open",
    "test:cloud": "cypress run --record --parallel --group 'Parallel Tests'",
    "test:cloud:chrome": "cypress run --record --browser chrome --group 'Chrome Tests'",
    "test:cloud:firefox": "cypress run --record --browser firefox --group 'Firefox Tests'",
    "test:cloud:electron": "cypress run --record --browser electron --group 'Electron Tests'"
  },
  "keywords": [
    "cypress",
    "testing",
    "e2e"
  ],
  "author": "Emile Francois",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^13.6.4",
    "start-server-and-test": "^2.0.3"
  }
}
